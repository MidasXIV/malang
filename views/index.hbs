<div class="col-md-7 ml-sm-auto col-lg-7 p-0">
    <div class="main-pane h-100 p-4">
        <h4 class="font-weight-light mb-1">Malang</h4>
        <h6 class="font-weight-light small-text">Dividend portfolio tracker - stimulator</h6>

        <hr>

        <form action="api/dividendHistory/" method="GET">
            <div class="form-row m-0">
                <div class="col-11 p-0">
                    <div class="form-group m-0">
                        <label for="inputSymbol" class="sr-only">Symbol</label>
                        <input type="text" class="form-control form-control-sm malang-form-input" id="inputSymbol" name="ticker"
                            placeholder="Search for Symbol">
                    </div>
                </div>
                <div class="col-1">
                    <button type="submit" class="btn btn-gray btn-sm btn-block m-0">
                        <svg class="bi bi-check icon" width="1em" height="1em" viewBox="0 0 16 16" fill="currentColor"
                            xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd"
                                d="M13.854 3.646a.5.5 0 010 .708l-7 7a.5.5 0 01-.708 0l-3.5-3.5a.5.5 0 11.708-.708L6.5 10.293l6.646-6.647a.5.5 0 01.708 0z"
                                clip-rule="evenodd" />
                        </svg>
                    </button>
                </div>
            </div>
        </form>


        <hr>

        <table class="table table-hover table-borderless malang-table">
            <thead>
                <tr>
                    <th>Ticker</th>
                    <th>Name</th>
                    <th>Price</th>
                    <th>P/E Ratio</th>
                    <th>EPS</th>
